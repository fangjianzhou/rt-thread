config RT_PCI_DW_HOST
    bool "DesignWare-based PCIe host"
    depends on RT_USING_PCI
    default n

config RT_PCI_DW_HOST_ROCKCHIP
    bool "Rockchip DesignWare PCIe host"
    depends on RT_PCI_DW_HOST
    select RT_USING_PHY
    select RT_USING_RESET
    select RT_MFD_SYSCON
    select RT_USING_REGULATOR
    default n
